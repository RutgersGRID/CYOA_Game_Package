<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="TutorialContainer" style="flex-grow: 1; background-color: rgb(255, 255, 255); width: 100%; height: 100%; opacity: 1; background-image: none; max-width: 1920px; max-height: 1080px; align-items: auto; align-content: auto; padding-top: 5%; padding-bottom: 5%; padding-left: 5%; padding-right: 5%; flex-direction: column; background-size: 80% 100%; justify-content: center; align-self: center;">
